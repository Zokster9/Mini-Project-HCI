<Window x:Class="MiniProject.TableWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiniProject"
        mc:Ignorable="d"
        Title="Table Data" Height="500" Width="800" MinHeight="100" MinWidth="300"
        Icon="/assets/money-bag.png">
    <Grid>
        <DataGrid x:Name="smaTable" ColumnWidth="*" ItemsSource="{Binding Path=SMA}" IsReadOnly="True" Margin="10" GridLinesVisibility="None"/>
    </Grid>
</Window>
